SELECT 
ITE.CODPROD
,PRO.DESCRPROD
,ITE.QTDNEG
,ITE.VLRUNIT
,ITE.VLRTOT
,ITE.CODLOCAL
,LOC.DESCRLOCAL
,ITE.OBSITEM
,ITE.CODVOL
FROM AD_REQUISICAOCOMPRAITENS ITE
INNER JOIN TGFPRO PRO ON PRO.CODPROD = ITE.CODPROD
INNER JOIN TGFLOC LOC ON LOC.CODLOCAL = ITE.CODLOCAL
WHERE ITE.IDINSTPRN = :IDINSTPRN
AND ITE.IDINSTTAR = :IDINSTTAR
ORDER BY ITE.CODREGISTRO ASC
